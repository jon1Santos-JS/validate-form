{
    "env": {
        "browser": true,
        "node": true
    },
    "extends": [
        "next/core-web-vitals",
        "eslint:recommended",
        "plugin:prettier/recommended",
        "plugin:@typescript-eslint/eslint-recommended",
        "plugin:@typescript-eslint/recommended",
        "plugin:react-hooks/recommended"
        ],
    "plugins": ["@typescript-eslint"],
    "rules": {
        "prettier/prettier": [
        "error",
        {
          "endOfLine": "auto"
        }
      ]
    }
}
